package multiex;

public class Main22 {

	public static void main(String[] args) {
		  int num[][] = {{3,2,4,5,6},{1,2,3}}; // 선언
		  int[][] num1 = new int [2][5]; // 선언과 생성을 한번에

		  int sum = 0;
		  for (int i =0; i < num.length; i++) {
			  for (int j =0; j < num1.length; j++) {
				  sum += num1[i][j];
		  int n = num1.length; // 배열의 행 개수
		  int u = num.length; // 배열의 열 개수
			  
				  System.out.println("평균치 : " + sum/(n*u));
			  }
			  
		  }
		  
	}
	}
	
	